{
    "name": "vasildakov/tms",
    "description": "Transportation Management System",
    "homepage": "https://github.com/vasildakov/tms",
    "type": "project",
    "authors": [
        {
            "name": "Vasil Dakov",
            "email": "vasildakov@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "roave/security-advisories": "dev-master",
        "zendframework/zend-expressive": "^1.0",
        "zendframework/zend-expressive-helpers": "^2.0",
        "zendframework/zend-expressive-twigrenderer": "^1.0",
        "zendframework/zend-stdlib": "^2.7 || ^3.0",
        "zendframework/zend-expressive-zendrouter": "^1.0",
        "zendframework/zend-expressive-zendviewrenderer": "^1.0",
        "zendframework/zend-servicemanager": "^2.7.3 || ^3.0",
        "zendframework/zend-authentication": "^2.5",
        "zendframework/zend-permissions-acl": "^2.6",
        "zendframework/zend-paginator": "^2.7",
        "zendframework/zend-hydrator": "^2.2",
        "zendframework/zend-filter": "^2.7",
        "zendframework/zend-form": "^2.8",
        "zendframework/zend-debug": "^2.5",
        "zendframework/zend-config": "^2.6",
        "zendframework/zend-http": "^2.5",
        "ocramius/proxy-manager": "^1.0 || ^2.0",
        "doctrine/orm": "^2.5",
        "doctrine/common": "^2.6",
        "doctrine/migrations": "^1.5",
        "doctrine/data-fixtures": "^1.1",
        "guzzlehttp/guzzle": "^6.2",
        "phpoffice/phpexcel": "^1.8",
        "pda/pheanstalk": "^3.1",
        "jms/serializer": "^1.3",
        "league/tactician": "^1.0",
        "league/tactician-container": "^1.0",
        "league/route": "^2.0",
        "league/event": "^2.1",
        "league/pipeline": "^0.3.0",
        "dasprid/container-interop-doctrine": "^0.2.2",
        "container-interop/container-interop": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7|^5.0",
        "squizlabs/php_codesniffer": "~2.3",
        "satooshi/php-coveralls": "~1.0",
        "phpunit/php-code-coverage": "^3.0|^4.0",
        "filp/whoops": "^1.1 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "Tms\\": "src/",
            "Application\\": "src/Application",
            "Domain\\": "src/Domain",
            "Infrastructure\\": "src/Infrastructure",
            "Presentation\\": "src/Presentation"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TmsTest\\": "tests/",
            "ApplicationTest\\": "tests/ApplicationTest",
            "DomainTest\\": "tests/DomainTest",
            "InfrastructureTest\\": "tests/InfrastructureTest",
            "PresentationTest\\": "tests/PresentationTest"
        }
    }
}
